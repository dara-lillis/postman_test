		{
			"name": "/v2/schedule",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "b509434d-461a-4525-a001-d78de469a6f4",
						"exec": [
							"var post_schedule_response = pm.response.json();",
							"",
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"create two schedule entries\", function () {",
							"    var expected_headers = [",
							"            \"facilityID\",",
							"            \"startDateTime\",",
							"            \"endDateTime\",",
							"            \"procedureName\",",
							"            \"operatingRoom\",",
							"            \"surgeon\",",
							"            \"serviceLine\",",
							"            \"facilityName\",",
							"            \"operatingRoomId\",",
							"            \"surgeonId\",",
							"            \"facilitySurgeonId\",",
							"            \"surgeryId\",",
							"            \"serviceLineId\",",
							"            \"procedureId\",",
							"            \"surgeryProcedureId\"",
							"        ]",
							"    // Validate headers.",
							"    pm.expect(post_schedule_response.data.headers, \"Validate response headers.\").to.have.same.members(expected_headers);",
							"",
							"    pm.expect(post_schedule_response.data.values.length, \"Two schedules entries created.\").to.equal(3)",
							"});"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "d552fa99-891d-411d-8b51-8e96ff6e9fc4",
						"exec": [
							"",
							"pm.variables.set(\"facilityID_1\", \"2022-08-22 08:08:01\");",
							"pm.variables.set(\"startDateTime_1\", \"2022-08-22 08:08:01\");",
							"pm.variables.set(\"endDateTime_1\", \"2022-08-22 10:01:00\");",
							"pm.variables.set(\"procedureName_1\", \"PLIF/TLIF/Decom/Bone Graft/Cage/Inst/1L\");",
							"pm.variables.set(\"operatingRoom_1\", \"10\");",
							"pm.variables.set(\"surgeon_1\", \"Ravi Yalamanchili\");",
							"pm.variables.set(\"serviceLine_1\", \"Orthopedics\");"
						],
						"type": "text/javascript"
					}
				}
			],
